{
  "name": "budgetzero",
  "version": "0.1.22",
  "scripts": {
    "dev": "vite",
    "tempdb": "pouchdb-server --in-memory --port 5984",
    "devdb": "run-p dev tempdb",
    "build": "vite build",
    "serve": "vite preview",
    "test:unit": "vue-cli-service test:unit --silent",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "electron:build": "vue-cli-service electron:build --publish always",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test-watch:unit": "vue-cli-service test:unit --watch --silent",
    "test:cypress": "cypress run",
    "test": "vitest",
    "vitest": "vitest run --coverage"
  },
  "main": "background.js",
  "dependencies": {
    "@riophae/vue-treeselect": "^0.4.0",
    "@sweetalert2/theme-borderless": "^4.0.5",
    "chart.js": "^3.3.2",
    "electron-updater": "^4.3.9",
    "file-saver": "^2.0.5",
    "flush-promises": "^1.0.2",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.29.1",
    "node-ofx-parser": "^0.5.1",
    "number-to-base64": "^1.0.5",
    "pouch-vue": "^0.3.5",
    "pouchdb": "^7.2.2",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-find": "^6.2.0",
    "pouchdb-live-find": "^0.4.0",
    "pouchdb-load": "^1.4.6",
    "save-dev": "^0.0.1-security",
    "sortablejs": "^1.13.0",
    "sweetalert": "^2.1.2",
    "validator": "^13.6.0",
    "vue": "^2.7.9",
    "vue-csv-import": "^3.3.10",
    "vue-fragment": "^1.6.0",
    "vue-json-tree-view": "^2.1.6",
    "vue-lodash": "^2.1.2",
    "vue-moment": "^4.1.0",
    "vue-papa-parse": "^3.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vue-select": "^3.11.2",
    "vue-sweetalert2": "^5.0.0",
    "vue-uuid": "^2.0.2",
    "vue2-filters": "^0.13.0",
    "vuedraggable": "^2.24.3",
    "vuefire": "^2.2.5",
    "vuejs-auto-complete": "^0.9.0",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.6.11",
    "vuetify-loader": "^1.7.3",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0",
    "worker-pouch": "^2.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.4",
    "@testing-library/vue": "^5.8.3",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/cli-plugin-e2e-cypress": "5.0.4",
    "@vue/test-utils": "1.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "cypress": "^12.15.0",
    "electron": "^16.0.6",
    "electron-builder": "22.10.5",
    "electron-devtools-installer": "3.2.0",
    "electron-icon-builder": "2.0.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "lint-staged": "11.2.6",
    "node-sass": "6.0.1",
    "npm-run-all": "^4.1.5",
    "pouchdb-server": "^4.2.0",
    "prettier": "^2.7.1",
    "sass": "^1.32.6",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-vue2": "^2.0.2",
    "vitest": "^0.22.1",
    "vue-cli-plugin-electron-builder": "2.1.1",
    "vue-cli-plugin-vuetify": "2.4.3",
    "vue-template-compiler": "^2.7.9",
    "yarn-upgrade-all": "0.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "func-names": "off",
      "no-console": "off",
      "linebreak-style": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ]
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "post-push": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BudgetZero/BudgetZero"
  }
}
